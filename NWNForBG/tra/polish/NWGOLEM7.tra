@0 = ~Qarth suclasa turth?~
@1 = ~Co mówisz?~
@2 = ~Do zobaczenia.~
@3 = ~Jam jest strażnikiem pierwszej pieczęci. Nikt nie przejdzie.~
@4 = ~Czym jest pierwsza pieczęć?~
@5 = ~This place is not for you. Only the masters can enter here.~ /*Oto twój Zhent. Co się tutaj dzieje?*/
@6 = ~Chcę się przedostać przez portal.~ 
@7 = ~Nikt nie przejdzie.~
@8 = ~Oto jest pierścień.~
@9 = ~Jestem jednym z Mistrzów. Daj mi przejść.~
@10 = ~Nie mówisz językiem Mistrzów. Pochodzisz z rasy niewolników. Nie przejdziesz.~
@11 = ~Ale ja został<e/a>m wysłan<y/a> przez Mistrzów. Mam ważne zadanie i muszę przejść.~
@12 = ~Nie masz pierścienia stwórcy. Niewolnicy posłani przez stwórców zawsze mają pierścień Gaxa.~
@13 = ~Gdzie mogę dostać taki pierścień?~
@14 = ~Niewolniku, nie odpowiem na więcej pytań.~
@15 = ~You can't get through, slave.~ /*Nie możesz powiedzieć niczego na pewno...*/
@16 = ~Możesz przejść.~
@17 = ~Skończyłem już z tobą.~
