@0 = ~Więc polujesz na Głównego Nadzorcę, hę? Wiesz, z czym się mierzysz?~ [NWKURD52]
@1 = ~To człowiek, jak każdy inny, może stracił zmysły przez zarazę.~
@2 = ~Właściwie miał<e/a>m nadzieję, że to jedna z istot z Waterdeep.~
@3 = ~Kim jesteś i czemu pomagasz Głównemu Nadzorcy?~
@4 = ~Nie jesteś godzien mojej uwagi. Zdychaj, śmieciu!~
@5 = ~Wyjąca Śmierć? Nie. Główny Nadzorca już nie jest człowiekiem, tylko skorupą. Coś wyżera jego mózg, coś patrzy jego oczami, coś go zmienia.~
@6 = ~Masz na to jakiś dowód?~
@7 = ~Co go opętało? Widziałeś to?~
@8 = ~W porządku, zostań tu. Poszukam Głównego Nadzorcy.~
@9 = ~Hrrr, Za dużo nie wiem. Tylko tyle, że on już nie człowiek, a pusta skorupa. Coś zżarło jego mózg. To coś patrzy jego oczami, zmienia go.~ 
@10 = ~Ja Kurdan Fenkt. Byłem Królem Kazamatów, do czasu, gdy pojawił się tu Główny Nadzorca. Teraz on Królem Kazamatów, a ja tylko jego sługa. Hrrr...~
@11 = ~Pozwoliłeś Głównemu Nadzorcy tak po prostu przejść? Dlaczego?~
@12 = ~Tu nic nie ma - byłeś panem niczego.~
@13 = ~Główny Nadzorca jest chory na Wyjącą Śmierć, to wszystko.~
@14 = ~Hrrr. Mam ci się poddać? Jeśli zginę, to przynajmniej jako Król Kazamatów. Stawaj do walki!~
@15 = ~Dlaczego on uwolnił więźniów? Dlaczego zmienił strażników w zombie? Dlaczego mi zaufał? Dlaczego? To już nie jest Alaefin, którego ja znał.~
@16 = ~Strażnicy zmienieni w zombie? O czym ty bredzisz?~
@17 = ~Taa, widziałem to coś. Jest małe jak pies, a śmierdzi gnijącymi porostami. Nie ma głowy, tylko wielki, otwarty mózg. To pewnie dlatego tak lubi je pożerać.~
@18 = ~To mi wygląda na robotę Pożeracza Intelektu, którego potrzebuje Lady Aribeth.~
@19 = ~Hrrr, nie zostaję tu. Jeśli Główny Nadzorca cię zabije, domyśli się, że ja cię przepuścił i zeżre mój mózg. Nie chcę tego. Idź już, zmierz się z nim. Pogadaj też z jego strażnikami, może odejdą i mózgowiec nie zdąży ich opanować. Tak, zrób to. Ja stąd znikam.~ 
@20 = ~On może zmienić ludzi w zombie. On zmienia nawet strażników. Mi też groził.~
@21 = ~Lepiej być Królem Kazamatów niż żadnym... Ilu znasz królów, no... powiedz?~
@22 = ~Wkrótce ich poznasz. Oni na dole, w jego siedlisku, bronią go. Karmią go. On straszy, że mnie też zmieni, jeśli ja nie będę mu służyć...~
@23 = ~To straszne. Czy można im jakoś pomóc?~
@24 = ~Nic o tym nie wiem. Główny Nadzorca to twarda sztuka i jeśli mózgojad dał sobie z nim radę, to musi być twardszy niż on. Jest naprawdę niebezpieczny.~
@25 = ~Ha! Może ty przemówisz im do rozumu. Pogadaj z wszystkimi, a może uratujesz ich przed mózgową kreaturą. Ona strasznie szybko się rozprzestrzenia. Taaa, może i tak. Pogadaj z ludźmi, może przekonasz ich, żeby odeszli. Jeśli ci się to nie uda, zabij bestię, która żeruje w Głównym Nadzorcy... i zrób to szybko.~ 
