@0 = ~Wyczuwam, że masz przy sobie Słowo Mocy. **syczy** Ale przybyłaś za późno, ciepłokrwista istoto. Moraga zbytnio urosła w siłę.~ [NWHAE165]
@1 = ~Ona czerpała siłę z dusz ofiar zarazy i rytuałów Maugrima. **syczy** Nie mogę dłużej powstrzymywać jej powrotu.~ [NWHAE166]
@2 = ~Wkrótce moja Władczyni posiądzie moc, dzięki której wyzwoli się z Kamienia Dusz bez użycia Słów Mocy.~ [NWHAE167]
@3 = ~A więc wszystko przepadło!~
@4 = ~Musi być coś, co możemy jeszcze zrobić.~
@5 = ~Wiedział<e/a>m, że nie można ci ufać!~
@6 = ~Jestem jedynie Niewolnicą Słów, nie mogę nic uczynić przeciwko rosnącej mocy mojej Królowej. **syczy** Lecz gdzie ja zawiodłam, ty możesz odnieść zwycięstwo.~ [NWHAE168]
@7 = ~Jak śmiesz! **syczy** Powrót Moragi oznacza dla mnie zniewolenie i męki, których nie jesteś sobie w stanie wyobrazić. W mym sercu nie ma miejsca na zdradę.~ [NWHAE170]
@8 = ~Możesz wejść do Kamienia Dusz i zetrzeć się z Moragą, nim odzyska ona pełną moc. Masz szansę zniszczyć ją, nim obudzi Pradawnych.~ [NWHAE169]
@9 = ~Wejdę do Kamienia Dusz i zniszczę Moragę!~
@10 = ~Jak mogę wejść do Kamienia Dusz?~
@11 = ~Nie zrobię tego!~
@12 = ~**syczy** Śpiesz się. Każda sekunda opóźnienia da Moradze większą siłę. **syczy** Życzę ci szczęścia, w imieniu nas wszystkich.~ [NWHAE113]

@14 = ~Kamień Dusz znajduje się w lochu pod tym zamkiem. **syczy** Powiedziałam już o tym ciepłokrwistemu, którego nazywacie Lordem Nasherem.~ [NWHAE114]
@15 = ~Więc Moraga zwycięży, a miasto, zwane przez ciebie Neverwinter - upadnie. Świat, który znasz, zniknie w ogniu powrotu Pradawnych.~ [NWHAE119]
@16 = ~Wokół Kamienia Dusz znajdziesz cztery postumenty. **syczy** Umieść na nich Słowa Mocy, a dostaniesz się do samego wnętrza Kamienia.~ [NWHAE115]
@17 = ~Gdzie są teraz Słowa Mocy?~
@18 = ~Trzy Słowa odzyskane poza miastem znajdowały się w posiadaniu ciepłokrwistego zwanego Lordem Nasherem. **syczy** Jeśli jest tak mądry, jak mówią, z pewnością odda je tobie.~ [NWHAE116]
@19 = ~Czwarte Słowo musisz odzyskać od samego Maugrima. **syczy** By wejść do Kamienia Dusz, potrzebujesz wszystkich czterech Słów.~ [NWHAE117]
@20 = ~To twój wybór, ja nie mogę nic zrobić. Ale to głupia decyzja. **syczy** Wróć do mnie, gdy odzyskasz rozum.~ [NWHAE120]
@21 = ~**syczy** Czekałam na ciebie, <CHARNAME>. Inni ciepłokrwiści są słabi. **syczy** Nie ufam im.~ [NWHAE121]
@22 = ~Są słabi, jak ci, których nazywasz Aribeth i Maugrimem. **syczy** Nie są w stanie przeciwstawić się Pradawnym, teraz gdy Moraga zaczyna się budzić w Kamieniu Dusz.~ [NWHAE122]
@23 = ~Maugrim wyzwolił magię Słów Mocy. **syczy** Pradawni obudzili się, a świat, który znasz, nigdy nie będzie taki sam.~ [NWHAE123]
@24 = ~Ależ Słowa Mocy są bezpieczne w zamku Never!~
@25 = ~Co robisz w tym więzieniu?~
@26 = ~Możesz powtórzyć, kim jesteś?~
@27 = ~Moraga? Kamień Dusz? Pradawni? Trochę to skomplikowane.~
@28 = ~Przynoszę czwarte Słowo Mocy!~
@29 = ~Przykro mi, nie mogę ci pomóc.~
@30 = ~Trzy Słowa znajdują się w tym zamku. **syczy** Ale wszystkich Słów jest cztery. Maugrim odnalazł ostatnie Słowo, najsilniejsze ze wszystkich.~ [NWHAE124]
@31 = ~**syczy** Przyszłam, by was ostrzec przed powrotem Moragi, ale ciepłokrwiści nie uwierzyli mi. **syczy** Wtrącili mnie do tego lochu.~ [NWHAE125]
@32 = ~**syczy** Przyszłam, by was ostrzec przed powrotem Moragi, ale ciepłokrwiści nie uwierzyli mi. **syczy** Wtrącili mnie do tego lochu.~ [NWHAE145]
@33 = ~Jestem Haedraline, Niewolnica Słów i służka Moragi, zanim została ona pochowana w Kamieniu Dusz. **syczy**~ [NWHAE147]
@34 = ~Dawno temu świat był gorący, a bujna dżungla porastała ziemię, dziś skutą lodem. Rasa Stwórców panowała, niewoląc ciepłokrwistych i inne, pomniejsze rasy.~ [NWHAE148]
@35 = ~Moraga była królową mego ludu - Pradawnych, Rasy Stwórców. **syczy** To ona stworzyła Kamień Dusz, gdy świat stawał się zimny, a śnieg pokrywał ziemię.~ [NWHAE149]
@36 = ~A więc nasz los został przesądzony. Maugrim obudzi jaszczurzą królową, ciepłokrwiści zostaną pokonani, a ja ponownie zostanę niewolnicą.~ [NWHAE160]
@37 = ~Nie marnuj czasu, próbując wyzwolić mnie z tego więzienia, to nie ma sensu. **syczy** Ważniejsze jest powstrzymanie Moragi, zanim Pradawni się obudzą, a ja raz jeszcze zostanę Niewolnicą Słów.~ [NWHAE126]
@38 = ~Co mam zrobić?~
@39 = ~Trzy Słowa znajdują się w tym zamku. **syczy** Ale wszystkich Słów jest cztery. Maugrim odnalazł to ostatnie.~ [NWHAE146]
@40 = ~Moraga użyła Słów, by pogrążyć swych ludzi w hibernacji, wiążąc ciepło podziemnych strumieni w magicznym Kamieniu Dusz - potem uwięziła w nim siebie i wszystkich swych pobratymców.~ [NWHAE150]
@41 = ~Dlaczego sama uwięziła się w tym Kamieniu Dusz?~
@42 = ~Co to ma ze mną wspólnego?~
@43 = ~Inni ciepłokrwiści są słabi duchem. Nie ufam im. **syczy** Jeśli twój rozum powróci, przyjdź do mnie. Tylko ty możesz pokonać Moragę!~ [NWHAE161]
@44 = ~Czwarte słowo jest blisko, czuję jego moc. **syczy** Maugrim i Aribeth przynieśli je tu, do miasta, które zwiesz Neverwinter.~ [NWHAE127]
@45 = ~Pradawni nie mogli przetrwać nadejścia zimowej burzy. **syczy** Moraga szukała schronienia, by móc się ukryć przed śmiercią, niesioną przez zimno i lód.~ [NWHAE151]
@46 = ~Coś obudziło ducha Moragi, choć jej ciało pozostaje uwięzione w Kamieniu Dusz. Porozumiewa się z Maugrimem przez projekcje, w taki sam sposób, jak ja rozmawiam teraz z tobą.~ [NWHAE157]
@47 = ~Przynieś mi Słowo. Jestem Haedraline, przywódczyni Niewolników Słów. **syczy** Mocą ostatniego Słowa będę mogła złamać potęgę Moragi i na zawsze zatrzymać ją w Kamieniu Dusz.~ [NWHAE128]
@48 = ~Wrócę, gdy tylko zdobędę Słowa Mocy!~
@49 = ~Skąd mam wiedzieć, że mogę ci wierzyć?~
@50 = ~Chcę zadać ci kilka pytań.~
@51 = ~W Kamieniu Dusz Moraga zbierała siły rok po roku, wiek po wieku. **syczy** Gdy jej moc osiągnie pełnię, Niewolnicy Słów obudzą królową, by zniszczyć wieczną zimę.~ [NWHAE152]
@52 = ~Więc uważasz, że jeśli Moraga się obudzi, to od razu zmieni Północ w tropikalną dżunglę?~ 
@53 = ~Moraga była głupia, powierzając swój los bandzie niewolników.~
@54 = ~W jaki sposób Maugrim wplątał się w to wszystko?~
@55 = ~Maugrim jej służy - marny pionek zaślepiony obietnicami nagrody i potęgi. **syczy** Zaraza była pierwszym krokiem do powrotu Moragi. Każda śmierć wzmacniała siłę Królowej, zbliżając ją do prawdziwego obudzenia.~ [NWHAE158]
@56 = ~Teraz Maugrim stara się obudzić Pradawnych za pomocą Słów Mocy. Jeśli mu się powiedzie, Moraga przyniesie Pólnocy straszliwą i nieodwracalną zagładę.~ [NWHAE159]
@57 = ~Musisz się pospieszyć, <CHARNAME>. **syczy** Moc Moragi rośnie z każdą godziną. Wkrótce nie będę mogła powstrzymać mej królowej przed obudzeniem jej ludzi.~ [NWHAE129]

@59 = ~Musisz mi zaufać, <CHARNAME>. **syczy** Jeśli tego nie zrobisz, Moraga obudzi Pradawnych, a Neverwinter zostanie zniszczone.~ [NWHAE130]
@60 = ~Twój lud zawsze był ciekawski. To pewnie klątwa waszej ciepłej krwi. **syczy** Nasz czas się kończy, ale odpowiem na twoje pytania.~ [NWHAE131]
@61 = ~Powiedz mi coś o Maugrimie.~
@62 = ~Opowiedz mi o Aribeth.~
@63 = ~Powiedz mi coś o sobie.~
@64 = ~Opowiedz mi o Moradze i Pradawnych.~
@65 = ~Opowiedz mi o Słowach Mocy i Kamieniu Dusz.~
@66 = ~Kamień Dusz został pogrzebany pod ulicami Neverwinter i nawet w stanie uśpienia promieniuje magicznym ciepłem, które pozwala na przetrwanie życia w mieście nawet podczas najsroższych zamieci.~ [NWHAE153]
@67 = ~Nie rozumiesz potęgi magii, która niegdyś przywiązała Niewolników do Słów Mocy! **syczy** Musieliśmy być jej posłuszni, nawet wtedy, gdy nasi Władcy spali. Ale Moraga nie doceniła nas.~ [NWHAE155]
@68 = ~Wyobraź sobie, co się stanie, gdy ta moc zostanie obudzona i wydostanie się na wolność. Ci, którzy przeżyją globalne ocieplenie staną się niewolnikami Stwórców, a ja ponownie przykuta do Słów Mocy. **syczy** Nie mogę na to pozwolić!~ [NWHAE154]
@69 = ~Opuszczeni na zimnym pustkowiu, urośliśmy w siłę wystarczającą, by wyrwać się z niewoli. **syczy** Ukryliśmy Słowa, by ich moc nigdy już nas nie zniewoliła, a nasi Panowie pozostali w uśpieniu.~ [NWHAE156]
@70 = ~Maugrim jest tylko marnym pionkiem. **syczy** Moraga wypacza jego umysł, by ostatecznie go zniszczyć.~ [NWHAE132]
@71 = ~Ta, którą zwiesz Aribeth, popadła w moc Moragi. Jakieś straszne wydarzenie sprawiło, że stała się podatna na zło, którym emanuje Moraga.~ [NWHAE134]
@72 = ~Jestem Haedraline, służka Moragi i ostatnia z Niewolników Słów. **syczy** Gdy Pradawni odeszli, zostaliśmy tutaj, przywiązani do naszej Władczyni za pomocą magii Słów.~ [NWHAE136]
@73 = ~Dawno temu, gdy elfy były jeszcze młode, świat był gorący i dżungla porastała to zmarznięte pustkowie. Był to czas mego ludu: Rasy Stwórców, Pradawnych.~ [NWHAE139]
@74 = ~Moraga stworzyła Kamień Dusz za pomocą magii Słów, **syczy** zamykając w nim energię ognia, podczas gdy świat zaczął się ochładzać.~ [NWHAE142]
@75 = ~Moja Królowa obiecała mu potęgę, lecz gdy tylko Pradawni się obudzą, zostanie zniewolony wraz z resztą pomniejszych ras.~ [NWHAE133]
@76 = ~Moja Królowa dotknęła jej umysłu poprzez sny, wzmacniając ciemną, zapiekłą nienawiść, która pożarła inne uczucia. Aribeth jest w tej chwili niewolnicą Moragi.~ [NWHAE135]
@77 = ~Zmuszeni do radzenia sobie w czasie wiecznej zimy, Niewolnicy Słów stali się silniejsi. **syczy** Zerwaliśmy magiczne więzy naszej upadłej Królowej i odzyskaliśmy wolność.~ [NWHAE137]
@78 = ~Ukryliśmy Słowa Mocy tak, by Pradawni nigdy się już nie obudzili, żeby nas zniewolić. **syczy** Lecz Maugrim naruszył Kamień Dusz i Moraga ponownie się budzi.~ [NWHAE138]
@79 = ~Pradawni zniewolili pomniejsze rasy i wszyscy ugięli się przed straszną siłą Moragi, naszej Królowej.~ [NWHAE140]
@80 = ~Lecz później nadeszła zima. Aby przeżyć, Moraga pogrążyła swych ludzi w głębokiej hibernacji wewnątrz Kamienia Dusz w oczekiwaniu na dzień, w którym mogłaby ponownie objąć rządy nad dawnym królestwem.~ [NWHAE141]
@81 = ~Moraga pogrążyła swych ludzi w głębokiej hibernacji wewnątrz Kamienia Dusz. **syczy** Podczas snu jej moc rosła i pewnego dnia z pewnością powstanie, by zakończyć wieczną zimę za pomocą magii Słów.~ [NWHAE143]
@82 = ~Jeśli Moraga w pełni się obudzi, wyzwoli przy tym prawdziwą moc Kamienia Dusz. Kraina lodu zmieni się w parną dżunglę, a Pradawni powrócą, by niewolić inne rasy, tak jak czynili to wieki temu.~ [NWHAE144]
@83 = ~Jesteś więc z powrotem. Musisz mnie wysłuchać, jeśli chcesz powstrzymać odrodzenie Moragi.~ [NWHAE162]
@84 = ~Widzę cię znów, **syczy** lecz nie wyczuwam obecności ostatniego Słowa Mocy.~ [NWHAE163]
@85 = ~Musisz odnaleźć Maugrima i czwarte Słowo Mocy. **syczy** Gdy będę miała wszystkie cztery Słowa, będę mogła powstrzymać powrót Moragi.~ [NWHAE164]
@86 = ~Moc Moragi jest zbyt wielka. Wkrótce wydostanie się ona z Kamienia Dusz bez pomocy Słów.~ [NWHAE111]
@87 = ~Musisz użyć Słów Mocy, by dostać się do Kamienia Dusz i zetrzeć się z nią, póki jej magia może zostać powstrzymana!~ [HAERD112]
